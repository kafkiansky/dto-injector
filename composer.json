{
    "name": "kafkiansky/dto-injector",
    "description": "Laravel library that help to inject dto in controller instead of request.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Zanfir",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Kafkiansky\\DtoInjector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kafkiansky\\DtoInjector\\Tests\\": "tests"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require": {
        "php": ">=8.0",
        "illuminate/container": "^8.17",
        "illuminate/validation": "^8.17",
        "illuminate/support": "^8.17",
        "illuminate/http": "^8.17",
        "illuminate/routing": "^8.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.1"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
